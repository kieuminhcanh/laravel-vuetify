{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "dev-all": "npm --section=server run dev && npm --section=client run dev",
        "dev-client": "npm --section=client run dev",
        "dev-server": "npm --section=server run dev",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-server": "npm --section=server run development -- --watch",
        "watch-client": "npm --section=client run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.19.2",
        "bufferutil": "^4.0.1",
        "cross-env": "^7.0.2",
        "deepmerge": "^4.2.2",
        "fibers": "^4.0.2",
        "laravel-mix": "^5.0.4",
        "laravel-mix-merge-manifest": "^0.1.2",
        "lodash": "^4.17.15",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "subscriptions-transport-ws": "^0.9.16",
        "utf-8-validate": "^5.0.2",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.3"
    },
    "dependencies": {
        "@vue/apollo-option": "^4.0.0-alpha.7",
        "@vue/apollo-ssr": "^4.0.0-alpha.6",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.16",
        "cross-fetch": "^3.0.4",
        "graphql": "^14.6.0",
        "isomorphic-fetch": "^2.2.1",
        "node-fetch": "^2.6.0",
        "vue": "^2.6.11",
        "vue-apollo": "^3.0.3",
        "vue-cli-plugin-apollo": "^0.21.3",
        "vue-router": "^3.1.6",
        "vue-server-renderer": "^2.6.11",
        "vuetify": "^2.6.10",
        "vuex": "^3.1.3",
        "vuex-router-sync": "^5.0.0"
    }
}
